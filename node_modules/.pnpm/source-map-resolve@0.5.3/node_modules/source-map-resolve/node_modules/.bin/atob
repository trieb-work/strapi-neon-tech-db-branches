#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tilman/Documents/Softwareentwicklung/arbeit/trieb-work-new-homepage-2023/strapi/src/plugins/strapi-neon-tech-db-branches/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/tilman/Documents/Softwareentwicklung/arbeit/trieb-work-new-homepage-2023/strapi/src/plugins/strapi-neon-tech-db-branches/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../atob@2.1.2/node_modules/atob/bin/atob.js" "$@"
else
  exec node  "$basedir/../../../../../atob@2.1.2/node_modules/atob/bin/atob.js" "$@"
fi
